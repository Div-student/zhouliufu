*{
  padding: 0;
  margin: 0
}
.header{
  width: 100%;
  height: 95px;
  background: rgba(0, 0, 0, .3);
  color: white
}
.header_content{
  text-align: center;
  width: 70%;
  height: 95px;
  line-height: 95px;
  margin: 0 auto;
}
.header_content>img{
  vertical-align: middle
}
.header_list{
  margin-left: 80px
}
.header_list>span{
  margin-left: 20px;
  padding-bottom: 10px;
  cursor: pointer;
}
.header_active{
  color: #00DCD8;
  border-bottom: solid 2px #00DCD8
}