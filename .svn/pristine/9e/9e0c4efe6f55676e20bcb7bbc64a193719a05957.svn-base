.botstyle{
    width: 100%;
    height: 355px;
    background: #434343;
}
.neirong{
    width: 1200px;
    height: 100%;
    margin: auto;
    display: flex;
}
.neirong > div {
    flex: 1;
    color: #F6F2F2;
    padding: 10px;
}
.neirong > div h3 {
    font-size: 20px;
}
.neirong > div a{
    color: #F6F2F2;
    text-decoration: none;
    display: block;
    margin: 10px 0px;
    font-size: 16px;
}
.neirong > div div{
    margin: 10px 0px;
}
.neirong > div div > span {
    display: inline-block;
}
.neirong > div div > span:last-child{
    color: #F6F2F2;
    font-size: 14px;
    width: 150px;
}
.neirong > div div > span:first-child {
    width: 30px;
    height: 30px;
    vertical-align: middle;
    text-align: center;
    margin-right: 10px;
}
.neirong > div:last-child>div:last-child>span:first-child {
    position: relative;
    bottom:20px;
}