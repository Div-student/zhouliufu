/*新闻*/
.news{
    width: 570px;
    height: 790px;
    position: fixed;
    right: 0px;
    top: 30%;
}
.news > .tou {
    width: 100%;
    height: 76px;
    background: #60D3D8;
    text-align: center;
    font-size: 45px;
    color: white;
    line-height: 76px;
    position: relative;
}
.news > .tou span {
    cursor: pointer;
}
.news > .tou span:after {
    content: '';
    display: inline-block;
    position: absolute;
    top: 38px;
    right: 22px;
    width: 40px;
    height: 3px;
    background: white;
    transform: rotate(45deg);
}
.news > .tou span:before {
    content: '';
    display: inline-block;
    position: absolute;
    top: 38px;
    right: 22px;
    width: 40px;
    height: 3px;
    background: white;
    transform: rotate(-45deg);
}
.news > .nei {
    width: 530px;
    height: 674px;
    padding: 20px;
    background: rgba(0, 0, 0, 0.4);
    word-wrap: break-word;
    color: #FFFFFF;
    font-size: 18px;
}