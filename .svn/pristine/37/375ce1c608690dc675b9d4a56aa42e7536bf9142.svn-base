.headstyle{
    width: 100%;
    background-image: url("../img/shouyebg.png");
    background-repeat: no-repeat;
    background-position: center;
    /* 当内容高度大于图片高度时，背景图像的位置相对于viewport固定 */
    /*background-attachment: fixed;*/
    /* 让背景图基于容器大小伸缩 */
    background-size: cover;
    height: 1000px;
}
/*遮罩*/
.zezao {
    width: 50%;
    height: 950px;
    background: rgba(0, 0, 0, 0.4);
    position: relative;
}
.xx {
    width: 412px;
    height: auto;
    position: absolute;
    top: 20%;
    right: 20px;
    color: #FFFFFF;
    text-align: right;
}
.xx > h3 {
    font-size: 38px;
    margin: 0px;
}
.xx > span {
    display: block;
    float: right;
}
.xx > .w1 {
    height: 48px;
    width: 230px;
    line-height: 48px;
    font-size: 20px;
    color: #00FFEA;
    border-bottom: 3px solid #00FFEA;
    margin: 10px 0px;
}
.xx > .w2 {
    text-align: left;
}
.xx > .w2 > i{
    color: #00FFEA;
}
.midd {
    height: 120px;
    position: absolute;
    bottom: 11%;
    right: -144px;
    color: #FFFFFF;
    text-align: center;
    font-size: 18px;
}
.midd > p {
    margin-bottom: 60px;
}
.midd > p span {
    color: #00FFEA;
}
/*外包*/
.waibao {
    height: 480px;
    width: 1200px;
    margin: auto;
    text-align: center;
    font-size: 42px;
    color: #000;
    padding-top: 40px;
}
.waibao > .zu {
    display: block;
    width: 600px;
    position: relative;
    margin: auto;
}
.waibao > .zu:after {
    content: '';
    display: block;
    width: 100px;
    height: 3px;
    background: #000;
    position: absolute;
    top: 25px;
    left: 70px;
}
.waibao > .zu:before {
    content: '';
    display: block;
    width: 100px;
    height: 3px;
    background: #000;
    position: absolute;
    top: 25px;
    right: 70px;
}
.waibao > .fu {
    display: block;
    font-size: 11px;
}
.waibao > img {
    margin-top: 60px;
}
/*了解更多*/
.moress{
    width: auto;
    height: 800px;
    background-image: url("../img/genduobg.png");
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
    padding-top: 320px;
}
.moress > div {
    width: 1200px;
    display: flex;
    margin: auto;
}
.moress > div > div {
    flex: 1;
    margin:  0px 32px;
    background: #1CC1C7;
    height: 400px;
    text-align: center;
}
.moress > div > div > img {
    margin-top: 30px;
}
.moress > div > div > span {
    display: block;
    height: 74px;
    background: white;
    color: #0A9BC6;
    font-size: 30px;
    line-height: 74px;
    margin-top: 85px;
}
.qiye{
    margin-top: 300px;
    background-image: url("../img/tiao.png");
    height: 230px;
    background-repeat: repeat-x;
}
.qiye > div {
    width: 1200px;
    margin: auto;
    text-align: center;
}
.qiye > div > img {
    margin-top: -7%;
}
.qiye > div > span {
    display: block;
    color: #1CC1C7;
    width: 150px;
    height: 53px;
    font-size: 20px;
    line-height: 53px;
    margin: auto;
    margin-top: 5%;
    cursor: pointer;
    border: 2px solid #1CC1C7;
}
/*技术*/
.jisu {
    width: 1200px;
    height: 540px;
    display: flex;
    margin: auto;
    margin-top: 60px;
}
.jisu > div {
    flex: 1;
    text-align: center;
    padding: 30px;
    position: relative;
}
.jisu > div > div {
    margin-top: 60px;
    margin-left: 18%;
}
.jisu > div > span {
    display: inline-block;
    width: 30px;
    height: 30px;
    line-height: 30px;
    color: white;
    border-radius: 50%;
    background: #1CC1C7;
    position: absolute;
    top: 27%;
    left: 46%;
}
.jisu > div > div > p {
    text-align: left;
    font-size: 10px;
    color: black;
}
