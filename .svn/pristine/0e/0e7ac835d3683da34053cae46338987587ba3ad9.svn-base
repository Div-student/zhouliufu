*{
    margin: 0;
    padding: 0;
}
/*背景*/
.enrollbg{
    background-image: url("../imgs/myshopping_img1.png");
    background-repeat: no-repeat;
    background-position: center center;
    /* 当内容高度大于图片高度时，背景图像的位置相对于viewport固定 */
    background-attachment: fixed;
    /* 让背景图基于容器大小伸缩 */
    background-size: cover;
    width: 100%;
    height: 650px;
    position: relative;
}
/*注册界面*/
.enrolly {
    width: 490px;
    height: auto;
    position: absolute;
    top: 5%;
    left: 10%;
}
/*头部选择*/
.enrollxz {
    width: 100%;
    height: 85px;
    display: flex;
}
.enrollxz > span {
    flex: 1;
    text-align: center;
    display: inline-block;
    line-height: 85px;
    background: #edebeb;
    color: #b2b2b2;
    font-size: 18px;
    cursor: pointer;
}
/*注册页面*/
.enrollym {
    width: 400px;
    height: 400px;
    background: white;
    padding: 45px;
}
.enrollym > div > ul li {
    height: 56px;
    border: 1px solid #ede8e8;
    margin-bottom: 15px;
    /*给图片，验证码留位置*/
    padding: 0 130px 0 45px;
    position: relative;
    list-style: none;
    color: #b2b2b2;
}
.enrollym > div > ul li > input {
    height: 56px;
    line-height: 56px;
    width: 100%;
    border: none;
    font-size: 16px;
    /*兼容*/
    box-sizing: border-box;
    padding: 5px;
    outline: none;
}
/*改变默认placeholder*/
::-webkit-input-placeholder {
    color: #aab2bd;
}
.enrollym > div > ul li > img {
    position: absolute;
    left: 5%;
    top: 33%;
}
.enrollym > div > ul li > .code {
    position: absolute;
    width: 130px;
    right: 0;
    top: 0;
    height: 56px;
    line-height: 56px;
    text-align: center;
    cursor: pointer;
    background: #f7f7f7;
    border-left: 1px solid #ede8e8;
    color: #dab866;
    display: inline-block;
}
.enrollym > div > ul li > .yanz {
    position: absolute;
    width: 130px;
    right: 0;
    top: 0;
    height: 56px;
    border-left: 1px solid #ede8e8;
    display: inline-block;
    background-image: url("../imgs/shopping_cart_pic25.png");
}
.enrollym > div > span {
    font-size: 14px;
    color: #666666;
    display: block;
    height: 20px;
    line-height: 20px;
}
.enrollym > div > a {
    display: inline-block;
    width: 100%;
    height: 60px;
    margin-top: 20px;
    text-align: center;
    background: #dab866;
    color: #fff;
    font-size: 18px;
    line-height: 60px;
    border-radius: 5px;
    text-decoration: none;
}
.enrollym > div > div {
    height: 80px;
    width: 100%;
    display: flex;
}
.enrollym > div > div > a {
    flex: 1;
    text-align: center;
    margin: auto;
}
.ymnone {
    display: none;
}
/*高亮*/
.xzgl {
    color: #666666!important;
    background: #ffffff!important;
}

